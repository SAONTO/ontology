<isms-all.ttl> rdf:type owl:Ontology .

isms:InformationSecurityTools rdf:type rdfs:Class ;
	rdfs:label "СЗИ"@ru ;
	rdfs:comment "Класс для средств защиты информации (СЗИ)"@ru .

isms:SCPI rdf:type rdfs:Class ;
	rdfs:subClassOf isms:InformationSecurityTools ;
	rdfs:label "СКЗИ"@ru ;
	rdfs:comment "Cистемы криптографической защиты информации (СКЗИ)"@ru .

isms:Specs rdf:type rdfs:Class ;
	rdfs:label "Категория ИС"@ru ;
	rdfs:comment "Класс для описаний, которые характеризуют информационные системы с технической и организационнной стороны, а также по требованиям регулятора"@ru .

isms:IS rdf:type rdfs:Class ;
	rdfs:label "ИС"@ru ;
	rdfs:comment "Класс информационных систем, в отношении которых формируются требования по защите информации"@ru .

isms:ISPD rdf:type rdfs:Class ;
	rdfs:subClassOf isms:IS ;
	rdfs:label "ИСПДн"@ru ;
	rdfs:comment "Информационные системы персональных данных (ИСПДн)"@ru .

isms:SIS rdf:type rdfs:Class ;
	rdfs:subClassOf isms:IS ;
	rdfs:label "ГИС"@ru ;
	rdfs:comment "Государственные информационные системы (ГИС)"@ru .

isms:C_SIS rdf:type rdf:Bag ;
	rdfs:label "ГИС"@ru ;
	rdfs:subClassOf isms:SIS ;
	rdfs:comment "Контейнер для ГИС"@ru .

isms:C_ISPD rdf:type rdf:Bag ;
	rdfs:label "ИСПДн"@ru ;
	rdfs:subClassOf isms:ISPD ;
	rdfs:comment "Контейнер для ИСПДн"@ru .

isms:ProtectiveMeasures rdf:type rdfs:Class ;
	rdfs:label "Мера защиты"@ru ;
	rdfs:comment "Меры и требования по обеспечению безопасности информации в информационных системах"@ru .

isms:LegalActs rdf:type rdfs:Class ;
	rdfs:label "НПА"@ru ;
	rdfs:сomment "Класс для нормативных правовых актов, определяющих меры и требования по обеспечению безопасности автоматизированных систем, а также порядок разработки, изготовления, и обеспечения эксплуатации автоматизированных систем"@ru .

isms:ThreatsOfSecurity rdf:type rdfs:Class ;
	rdfs:label "УБИ"@ru ;
	rdfs:сomment "Угрозы безопасности информации"@ru .

isms:possibleImplementation rdf:type rdf:Property ;
	rdfs:label "В ИС возможна рализация УБИ"@ru ;
	rdfs:comment "В результате наличия субъекта (техническая и эксплуатационная характеристика ИС) возможна реализация объекта (угроза безопасности информации)"@ru ;
	rdfs:domain isms:IS ;
	rdfs:range isms:ThreatsOfSecurity .

isms:acting rdf:type rdf:Property ;
	rdfs:label "Действующий/недействующий НПА"@ru ;
	rdfs:сomment "Информация о статусе НПА: действующий - true, отмененный - false"@ru ;
	rdfs:domain isms:LegalActs ;
	rdfs:range xsd:boolean .

isms:isNecessaryFor rdf:type rdf:Property ;
	rdfs:label "Мера необходима для ИС"@ru ;
	rdfs:comment "Субъект (мера, сведения) требуется для (реализации системы защиты в информационной системе, оформления аттестационных документов, мера защиты)"@ru ;
	rdfs:domain isms:ProtectiveMeasures ;
	rdfs:range isms:IS .

isms:helpsTo rdf:type rdf:Property ;
	rdfs:label "Мера может применяться для ИС"@ru ;
	rdfs:comment "Субъект (мера, сведения) может использоваться для (реализации системы защиты в информационной системе, оформления аттестационных документов, мера защиты)"@ru ;
	rdfs:domain isms:ProtectiveMeasures ;
	rdfs:range isms:IS .

isms:isReducedDue rdf:type rdf:Property ;
	rdfs:label "УБИ блокируется мерой"@ru ;
	rdfs:сomment "Субъект (УБИ) блокируется или снижается вероятность его реализации при наличии объекта (мера по обеспечению информационной безопасности)"@ru ;
	rdfs:domain isms:ThreatsOfSecurity ;
	rdfs:range isms:ProtectiveMeasures .

isms:spec rdf:type rdf:Property ;
	rdfs:label "ИС описывается категорией"@ru ;
	rdfs:comment "Субъект (компьютерная система) описывается объектом (категория описания системы)"@ru ;
	rdfs:domain isms:IS ;
	rdfs:range isms:Specs .

isms:forMeasure rdf:type rdf:Property ;
	rdfs:label "СЗИ для реализации меры"@ru ;
	rdfs:comment "Субъект (СЗИ) предназначен для реализации объекта (мера по защите информации)"@ru ;
	rdfs:domain isms:InformationSecurityTools ;
	rdfs:range isms:ProtectiveMeasures .

isms:forThreat rdf:type rdf:Property ;
	rdfs:label "СЗИ для блокирования УБИ"@ru ;
	rdfs:comment "Субъект (СЗИ) предназначен для блокирования объекта (УБИ) или снижения вероятности его реализации"@ru ;
	rdfs:domain isms:InformationSecurityTools ;
	rdfs:range isms:ThreatsOfSecurity .







